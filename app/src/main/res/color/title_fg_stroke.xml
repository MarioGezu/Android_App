<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Focused state -->
    <item android:color="@color/title_fg" android:state_focused="true" />
    <!-- Enabled (default) state -->
    <item android:color="@color/title_fg" android:state_enabled="true"/>
    <!-- Disabled state -->
    <item android:color="@color/title_fg" android:state_enabled="false"/>
</selector>
